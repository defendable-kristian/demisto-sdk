{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: create graph",
            "type": "python",
            "request": "launch",
            "module": "demisto_sdk",
            "cwd": "${env:CONTENT}", // Predefine the CONTENT variable in your environment first!
            "args": [
                "create-content-graph",
            ]
        },
        {
            "name": "Python: create artifacts",
            "type": "python",
            "request": "launch",
            "module": "demisto_sdk",
            "cwd": "${env:CONTENT}", // Predefine the CONTENT variable in your environment first!
            "args": [
                "create-content-artifacts-v2",
            ]
        },
        {
            "name": "Python: Lint AbuseDB with logs",
            "type": "python",
            "request": "launch",
            "module": "demisto_sdk",
            "cwd": "${env:CONTENT}", // Predefine the CONTENT variable in your environment first!
            "args": [
                "lint",
                "-i",
                "${env:CONTENT}/Packs/AbuseDB/Integrations/AbuseDB",
                "-j",
                "${env:CONTENT}/.demisto-sdk-report"
            ]
        }
    ]
}