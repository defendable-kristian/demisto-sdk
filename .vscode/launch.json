{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: create content graph",
            "type": "python",
            "request": "launch",
            "module": "demisto_sdk",
            "cwd": "${env:CONTENT}", // Predefine the CONTENT variable in your environment first!
            "args": [
                "create-content-graph",
                "--keep-service"
            ]
        },
        {
            "name": "Python: load content graph",
            "type": "python",
            "request": "launch",
            "module": "demisto_sdk",
            "cwd": "${env:CONTENT}", // Predefine the CONTENT variable in your environment first!
            "args": [
                "load-content-graph",
                "--keep-service"
            ]
        },
        {
            "name": "Python: Lint AbuseDB with logs",
            "type": "python",
            "request": "launch",
            "module": "demisto_sdk",
            "cwd": "${env:CONTENT}", // Predefine the CONTENT variable in your environment first!
            "args": [
                "lint",
                "-i",
                "${env:CONTENT}/Packs/AbuseDB/Integrations/AbuseDB",
                "-j",
                "${env:CONTENT}/.demisto-sdk-report"
            ]
        }
    ]
}